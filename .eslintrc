{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true, "parser": "flow" }],
    "import/extensions": 0,
    "no-undef": 0,
    "react/jsx-filename-extension": 0,
    "no-unused-vars": 0,
    "no-multi-assign": 0,
    "func-names": 0,
    "prefer-spread": 0,
    "prefer-rest-params": 0,
    "one-var": 0,
    "prefer-destructuring": 0,
    "no-console": 0
  }
}
